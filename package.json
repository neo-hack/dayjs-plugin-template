{
  "name": "@aiou/dayjs-plugin-template",
  "version": "1.1.1",
  "packageManager": "pnpm@7.29.3",
  "description": "dayjs plugin template",
  "keywords": [
    "dayjs",
    "dayjs-plugin",
    "plugin",
    "template",
    "aiou"
  ],
  "license": "MIT",
  "homepage": "https://github.com/neo-hack/dayjs-plugin-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neo-hack/dayjs-plugin-template.git"
  },
  "bugs": {
    "url": "https://github.com/neo-hack/dayjs-plugin-template/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": {
    "name": "JW",
    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "src/typings.d.ts",
  "scripts": {
    "build:dts": "ttsc",
    "build:lib": "rimraf lib && rollup -c",
    "build": "pnpm run build:dts && pnpm run build:lib",
    "dev": "rollup -c -w --silent",
    "test": "vitest",
    "pretest": "pnpm run build",
    "ci:publish": "pnpm run build && pnpm changeset publish",
    "ci:version": "pnpm changeset version",
    "ci:snapshot": "pnpm changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpm changeset publish --tag beta",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": ["eslint --fix"]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "dayjs": "*"
  },
  "devDependencies": {
    "@aiou/eslint-config": "0.11.1",
    "@changesets/cli": "^2.16.0",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/node": "^20.5.9",
    "copyfiles": "^2.4.1",
    "cz-emoji": "^1.3.1",
    "dayjs": "^1.9.7",
    "eslint": "8.48.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.1",
    "rimraf": "3.0.2",
    "rollup": "3.28.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-node-externals": "^6.1.1",
    "ttypescript": "1.5.15",
    "typescript": "^4.3.5",
    "typescript-transform-paths": "^3.4.6",
    "vitest": "^0.34.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
